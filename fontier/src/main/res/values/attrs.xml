<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FontView">
        <attr name="fontName"/>
    </declare-styleable>
    <attr name="fontName" format="enum">
        <enum name="Poppins_Regular" value="1"/>
        <enum name="Poppins_Light" value="2"/>
        <enum name="Poppins_ExtraLight" value="3"/>
        <enum name="Poppins_Bold" value="4"/>
        <enum name="Poppins_Medium" value="5"/>
        <enum name="MerrieWeather_Regular" value="6"/>
        <enum name="MerrieWeather_Bold" value="7"/>
        <enum name="MerrieWeather_Light" value="8"/>
        <enum name="Dayrom" value="9"/>
        <enum name="DayromX" value="10"/>
    </attr>
    <declare-styleable name="CircleImage">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_circle_background_color" format="color" />
    </declare-styleable>

    <declare-styleable name="CheckView">
        <attr name="backgroundView" format="reference"/>
        <attr name="srcView" format="reference"/>
        <attr name="textView" format="string|reference"/>
        <attr name="tickView" format="reference"/>
        <attr name="subtitle" format="string|reference"/>
        <attr name="is_tick" format="boolean"/>
    </declare-styleable>
</resources>